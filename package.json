{
  "name": "@trufnetwork/kwil-js",
  "version": "0.9.5",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist-esm/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trufnetwork/kwil-js.git"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist-esm/**/*.js",
    "dist-esm/**/*.d.ts",
    "dist-esm/package.json",
    "!dist/**/*.map",
    "!dist/**/*.test.js",
    "!dist/**/*.test.ts",
    "!dist-esm/**/*.map",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist/* dist-esm/*",
    "build": "pnpm run clean && tsc && tsc -p tsconfig.esm.json && node -e \"const fs=require('fs');fs.mkdirSync('dist-esm',{recursive:true});fs.writeFileSync('dist-esm/package.json','{\\\"type\\\":\\\"module\\\"}\\n');\"",
    "type-check:esm": "tsc -p tsconfig.esm.json --noEmit",
    "commit": "git add . && git-cz",
    "prepare": "husky install",
    "release": "standard-version",
    "pack_pre": "rimraf ./kwil*.tgz",
    "pack_post": "copyfiles ./kwil*.tgz ./pkg && rimraf ./kwil*.tgz",
    "pack": "pnpm run build && pnpm run pack_pre && pnpm pack && pnpm run pack_post",
    "integration": "jest --runInBand --testPathPattern=tests/integration --testTimeout=30000",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "author": "Kwil, Inc. <luke@kwil.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.11.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "near-api-js": "^2.1.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "scrypt-js": "^3.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "ethers": "^6.9.1",
    "jssha": "^3.2.0",
    "long": "^5.2.1",
    "lru-cache": "^11.1.0",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "husky": {
      "hooks": {
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    }
  }
}
